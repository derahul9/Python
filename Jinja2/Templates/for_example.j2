###Single for loop
{%- for port_number in range(2) %}
interface Gig0/1/{{ port_number }}
 switchport access vlan 400
 switchport mode access
!
{%- endfor %}

##Nested for loop
{%- for slot in range(1, 3) %}
  {%- for port_number in range(1, 3) %}
interface gig 0/{{ slot }}/{{ port_number }}
  {%- endfor %}
{%- endfor %}

